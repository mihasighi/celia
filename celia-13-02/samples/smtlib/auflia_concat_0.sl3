(set-logic AUFLIA)
(define-sort Node () (Array Int Int) )
(declare-fun len (Node) Int)
(declare-fun Gall (Int Node) Bool)
(declare-fun Gle2 (Int Int Node) Bool)
(declare-fun Gsucc2 (Int Int Node) Bool)
(declare-fun Gfst (Int Node) Bool)
(declare-fun Glst (Int Node) Bool)
(declare-fun Geq2 (Int Node Int Node) Bool)
(declare-fun n0 () Node)
(declare-fun n1 () Node)
(declare-fun n2 () Node)
(declare-fun n3 () Node)
(assert (and 		(= 0 (- (+  (len n2)  0 )  (+ 1 0 ) ) )
		(<= 0 (- (+  (select n2 0)  0 )  (+  (select n1 0)  0 ) ) )
		(<= 0 (- (+  (select n3 0)  0 )  (+  (select n2 0)  0 ) ) )
		(<= 0 (- (+  (len n3)  0 )  (+ 1 0 ) ) )
		(<= 0 (- (+  (len n1)  0 )  (+ 1 0 ) ) )

		(forall ((?y1 Int) (?y2 Int))
		(=> (Gle2 ?y1 ?y2 n1)
		(and 
		(= 0 (- (+  (len n2)  0 )  (+ 1 0 ) ) )
		(<= 0 (- (+  (select n2 0)  0 )  (+  (select n1 0)  0 ) ) )
		(<= 0 (- (+  (select n3 0)  0 )  (+  (select n2 0)  0 ) ) )
		(<= 0 (- (+  ?y2  0 )  (+  ?y1  0 ) ) )
		(<= 0 (- (+   (select n1 ?y2)   0 )  (+   (select n1 ?y1)   0 ) ) )
		(<= 0 (- (+  (len n3)  0 )  (+ 1 0 ) ) )
		(<= 0 (- (+  (len n1)  0 )  (+  ?y2 1) ) )
		(<= 0 (- (+  (len n1)  0 )  (+ 2 0 ) ) )
		)
) ) 
		(forall ((?y1 Int))
		(=> (Gall ?y1 n1)
		(and 
		(= 0 (- (+  (len n2)  0 )  (+ 1 0 ) ) )
		(<= 0 (- (+   (select n1 ?y1)   0 )  (+  (select n1 0)  0 ) ) )
		(<= 0 (- (+  (select n3 0)  0 )  (+  (select n2 0)  0 ) ) )
		(<= 0 (- (+  (len n3)  0 )  (+ 1 0 ) ) )
		(<= 0 (- (+  (len n1)  0 )  (+  ?y1 1) ) )
		(<= 0 (- (+  (len n1)  0 )  (+ 2 0 ) ) )
		(<= 0 (- (+  (select n2 0)  0 )  (+   (select n1 ?y1)   0 ) ) )
		)
) ) 
		(forall ((?y1 Int))
		(=> (Gall ?y1 n3)
		(and 
		(= 0 (- (+   (select n3 ?y1)   0 )  (+  (select n3 0)  ?y1 ) ) )
		(= 0 (- (+  (len n2)  0 )  (+ 1 0 ) ) )
		(<= 0 (- (+  (select n2 0)  0 )  (+  (select n1 0)  0 ) ) )
		(<= 0 (- (+  (select n3 0)  0 )  (+  (select n2 0)  0 ) ) )
		(<= 0 (- (+  (len n3)  0 )  (+  ?y1 1) ) )
		(<= 0 (- (+  (len n3)  0 )  (+ 2 0 ) ) )
		(<= 0 (- (+  (len n1)  0 )  (+ 1 0 ) ) )
		)
) ) 
) )
(assert (not (and 		(= 0 (- (+  (len n2)  0 )  (+ 1 0 ) ) )
		(<= 0 (- (+  (select n2 0)  0 )  (+  (select n1 0)  0 ) ) )
		(<= 0 (- (+  (select n3 0)  0 )  (+  (select n2 0)  0 ) ) )
		(<= 0 (- (+  (len n3)  0 )  (+ 1 0 ) ) )
		(<= 0 (- (+  (len n1)  0 )  (+ 1 0 ) ) )

	(forall ((?y1 Int) (?y2 Int))
		(=> (Gle2 ?y1 ?y2 n1)
		     (and 		(= 0 (- (+  (len n2)  0 )  (+ 1 0 ) ) )
		(<= 0 (- (+  (select n2 0)  0 )  (+  (select n1 0)  0 ) ) )
		(<= 0 (- (+  (select n3 0)  0 )  (+  (select n2 0)  0 ) ) )
		(<= 0 (- (+  ?y2  0 )  (+  ?y1  0 ) ) )
		(<= 0 (- (+   (select n1 ?y2)   0 )  (+   (select n1 ?y1)   0 ) ) )
		(<= 0 (- (+  (len n3)  0 )  (+ 1 0 ) ) )
		(<= 0 (- (+  (len n1)  0 )  (+  ?y2 1) ) )
		(<= 0 (- (+  (len n1)  0 )  (+ 2 0 ) ) )
)
		))

	(forall ((?y1 Int))
		(=> (Gall ?y1 n1)
		     (and 		(= 0 (- (+  (len n2)  0 )  (+ 1 0 ) ) )
		(<= 0 (- (+   (select n1 ?y1)   0 )  (+  (select n1 0)  0 ) ) )
		(<= 0 (- (+  (select n3 0)  0 )  (+  (select n2 0)  0 ) ) )
		(<= 0 (- (+  (len n3)  0 )  (+ 1 0 ) ) )
		(<= 0 (- (+  (len n1)  0 )  (+  ?y1 1) ) )
		(<= 0 (- (+  (len n1)  0 )  (+ 2 0 ) ) )
		(<= 0 (- (+  (select n2 0)  0 )  (+   (select n1 ?y1)   0 ) ) )
)
		))

	(forall ((?y1 Int))
		(=> (Gall ?y1 n3)
		     (and 		(= 0 (- (+   (select n3 ?y1)   0 )  (+  (select n3 0)  ?y1 ) ) )
		(= 0 (- (+  (len n2)  0 )  (+ 1 0 ) ) )
		(<= 0 (- (+  (select n2 0)  0 )  (+  (select n1 0)  0 ) ) )
		(<= 0 (- (+  (select n3 0)  0 )  (+  (select n2 0)  0 ) ) )
		(<= 0 (- (+  (len n3)  0 )  (+  ?y1 1) ) )
		(<= 0 (- (+  (len n3)  0 )  (+ 2 0 ) ) )
		(<= 0 (- (+  (len n1)  0 )  (+ 1 0 ) ) )
)
		))
)))
(check-sat)
