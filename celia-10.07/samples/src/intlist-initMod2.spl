var _data:real, _free:real, _len:real, 
    _new:real, _next:real, _null:real,
    x:real, xi:real, y:real,
    _l:int, _k:int, S:int;
begin
  assume (x == 0); 
  xi = _null; y = _null;
   _k = 0;
  xi = x;
  while xi != _null do
    if (_k<=0) then
      xi = 0 / _data;
      _k = 1;
    else
      xi = 1 / _data;
