var _data:real, _free:real, _len:real, 
    _new:real, _next:real, _null:real,
    x:real, xi:real, y:real,
    _l:int, _k: int, S:int, m:int;
begin
  assume (x == 0); 
  xi = _null; y = _null;
  m = 0;
  xi = x;
  while xi != _null do
    xi = m / _data;
    y = xi*_next;
    xi = _null;
    xi = y;
    y = _null;
    m = m+1;
  done;
end
