var _data:real, _free:real, _len:real, 
    _new:real, _next:real, _null:real,
    x:real, xi:real, y:real,
    _l:int, _k:int, S:int, sum:int;
begin
  assume (x == 0); 
  xi = _null; y = _null;
  xi = x;
  sum = 0;
  while xi != _null do
    sum = sum + xi * _data;
    y = xi*_next;
    xi = _null;
    xi = y;
    y = _null;
  done;
end
